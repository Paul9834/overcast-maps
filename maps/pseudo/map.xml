</rules>
<teams>
   <team color="dark red" max="8">Red</team>
   <team color="blue" max="8">Blue</team>
</teams>
<kits>
   <kit name="anti-kb">
      <knockback-reduction>0.5</knockback-reduction>
   </kit>
</kits>
<spawns>
   <spawn team="red" kit="anti-kb" yaw="-90">
      <cylinder base="-36,1,0" radius="1" height="0"/>
   </spawn>
   <spawn team="blue" kit="anti-kb" yaw="90">
      <cylinder base="37,1,0" radius="1" height="0"/>
   </spawn>
   <default yaw="0">
      <cylinder base="0,1,-21" radius="1" height="0"/>
   </default>
</spawns>
</map>
