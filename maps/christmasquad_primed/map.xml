<rules>
    <rule>Do not attempt to hide.</rule>
</rules>
<spawns>
    <spawn team="red" kit="red" yaw="90">
        <cuboid min="20,61,-72" max="32,61,-84"/>
    </spawn>
    <spawn team="blue" kit="blue" yaw="180">
        <cuboid min="20,61,-6" max="32,61,6"/>
    </spawn>
    <spawn team="green" kit="green" yaw="0">
        <cuboid min="-58,61,-72" max="-46,61,-84"/>
    </spawn>
    <spawn team="yellow" kit="yellow" yaw="-90">
        <cuboid min="-58,61,6" max="-46,61,-6"/>
    </spawn>
    <default yaw="0" pitch="20">
        <point>-13,37,-39</point>
    </default>
</spawns>
<blitz>
    <time>10m</time>
    <lives>1</lives>
</blitz>
<teams>
    <team color="dark red" max="10" max-overfill="12">Red</team>
    <team color="blue" max="10" max-overfill="12">Blue</team>
    <team color="green" max="10" max-overfill="12">Green</team>
    <team color="yellow" max="10" max-overfill="12">Yellow</team>
</teams>
